{
  "name": "openvtpk",
  "description": "inspect and transform vector tile packages",
  "version": "0.2.0",
  "author": "thomas.halwax@syncpoint.io",
  "bin": {
    "openvtpk": "./bin/run"
  },
  "bugs": "https://github.com/ThomasHalwax/openvtpk/issues",
  "dependencies": {
    "@mapbox/mbtiles": "^0.11.0",
    "@mapbox/vector-tile": "^1.3.1",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@syncpoint/compact-cache-bundle": "^0.3.0",
    "bluebird": "^3.7.1",
    "pbf": "^3.2.1",
    "proj4": "^2.5.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/ThomasHalwax/openvtpk",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "openvtpk",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ThomasHalwax/openvtpk",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
